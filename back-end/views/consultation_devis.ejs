</div>

<h1>Consultation devis</h1>

<!-- message d'information -->
<% if(locals.flash && locals.flash.error) { %> 
<div>
    <%= flash.error %>
</div>
<% } %> 

<% if(locals.flash && locals.flash.success) { %> 
<div>
    <%= flash.success %>
</div>
<% } %> 

<div>
    <h3>Projets</h3>

    <% for (projet of projets) { %> 

        <div>
            <a href="/consultation_devis_detail?id=<%= projet.projet_reference %>"><%= projet.gamme_id %> + <%= projet.projet_date %> + <%= projet.projet_client %></a>
        </div>

    <% } %> 

</div>


<%- include('footer') %> 